                        </goals>
                        <configuration>
                            <files>
                                <file>target/mijao.osgi.version.txt</file>
                            </files>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-release-plugin</artifactId>
                <version>2.4.2</version>
                <configuration>
                    <useReleaseProfile>false</useReleaseProfile>
                    <preparationGoals>clean install</preparationGoals>
                    <goals>deploy</goals>
                    <arguments>-Prelease,deploy</arguments>
                    <autoVersionSubmodules>true</autoVersionSubmodules>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.felix</groupId>
                <artifactId>maven-bundle-plugin</artifactId>
                <extensions>true</extensions>
                <inherited>true</inherited>
                <configuration>
                    <excludeDependencies>${mijao.osgi.exclude.dependencies}</excludeDependencies>
                    <instructions>
                        <Bundle-Name>mijao-${project.artifactId}</Bundle-Name>
                        <Bundle-SymbolicName>${mijao.osgi.symbolic.name}</Bundle-SymbolicName>
                        <Bundle-Activator>${mijao.osgi.activator}</Bundle-Activator>
                        <Export-Package>${mijao.osgi.export}</Export-Package>
                        <Import-Package>${mijao.osgi.import}</Import-Package>
                        <DynamicImport-Package>${mijao.osgi.dynamic}</DynamicImport-Package>
                        <Private-Package>${mijao.osgi.private.pkg}</Private-Package>
                        <Implementation-Title>Mijao</Implementation-Title>
                        <Implementation-Version>${project.version}</Implementation-Version>
                        <_versionpolicy>${mijao.osgi.import.default.version}</_versionpolicy>
                        <_failok>${mijao.osgi.failok}</_failok>
                        <Embed-Dependency>${mijao.osgi.embed}</Embed-Dependency>
                        <Mutator-Classpath>${mijao.osgi.mutator}</Mutator-Classpath>
                        <Fragment-Host>${mijao.osgi.fragmenthost}</Fragment-Host>
                                               <Embed-Dependency>${mijao.osgi.embed.dependency}</Embed-Dependency>

                                               <Require-Bundle>${mijao.osgi.require.bundles}</Require-Bundle>
                                               <Spring-Context>${mijao.osgi.spring.context}</Spring-Context>

                                               <Meta-Persistence>${mijao.osgi.jpa.context}</Meta-Persistence>
                    </instructions>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-resources-plugin</artifactId>
                <version>${maven.resources.plugin.version}</version>
